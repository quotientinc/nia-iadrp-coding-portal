{% for subfield, subitem in item %}
  {% if subitem -%}
    <div class="double-field-{{ subfield }}">
      {%- if field_settings.storage[subfield].type == 'text' -%}
        {{- subitem|nl2br -}}
      {%- elseif field_settings.storage[subfield].type == 'float' -%}
        {{- subitem*100|number_format -}}%
      {%- else -%}
        {{- subitem -}}
      {%- endif -%}
    </div>
  {% endif %}
{% endfor %}
